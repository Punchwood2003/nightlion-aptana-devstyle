	"semanticHighlighting": true,
	"semanticTokenColors": {
		"class": "#CAE682", // Classes
		"interface": "#CAE682", // Interfaces
		"templateType": "#CAE682", // Template Type
		"type": "#CAE682", // Type
		"class.abstract": "#3EABE6", // Abstract Classes
		"*.deprecated": "#FFFFFF", // Deprecated Members
		"*.importDeclaration": "#FFFFFF", // Imports
		"*.abstract.importDeclaration": "#FFFFFF", // Abstract Imports
		"*.constructor": "#DFBE95", // Constructors
		"function": "#DFBE95", // Functions
		"method": "#DFBE95", // Methods
		"function.static": "#C4C4B7", // Static Functions
		"method.static": "#C4C4B7", // Static Methods
		"function.declaration": "#DFBE95", // Function Declaration (e.g. in a class)
		"method.declaration": "#DFBE95", // Method Declaration
		"parameter": "#D4C4A9", // Parameters
		"variable": "#D4C4A9", // Variables
		"variable.declaration": "#D4C4A9", // Local Variable Declaratione
		"variable.static": "#93A2CC", // Static Variable
		"variable.statis.readonly": "#53DCCD", // Static Readonly Variable
		"property": "#B3B784", // Properties
		"property.static": "#93A2CC", // Static Property
		"property.static.readonly": "#53DCCD", // Static Readonly Property
		"enum": "#E2E2E2", // Enum
		"enumMember": "#53DCCD", // Enum Member
		"annotationMember": "#EB4B50", // Annotation Member
		"annotation": "#808080", // Annotation
		"typeParameter": "#FFFFFF", // Type Parameter
	},
	"tokenColors": [
		/** 
		 * ###########################
		 * # KEY WORDS AND OPERATORS #
		 * ###########################
		 */
		{
			"name": "Keywords, Primitive Data Types, and Storage Modifiers",
			"scope": [
				"keyword",
				"storage.type.primitive",
				"storage.modifier - storage.modifier.import",
				"storage.modifier.java",
				"variable.language.this",
				"storage.modifier.extends",
				"storage.modifier.implements",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#8DCBE2"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator.arithmetic",
				"keyword.operator.assignment",
				"keyword.operator.bitwise",
				"keyword.operator.logical",
				"keyword.operator.relational",
				"keyword.operator.unary",
				"keyword.operator.increment",
				"keyword.operator.decrement",
				"keyword.operator.increment-decrement",
				"keyword.operator.comparison",
				"keyword.operator.ternary",
				"keyword.control.ternary",
				"punctuation.section",
				"meta.operator"
			],
			"settings": {
				"foreground": "#F0EFD0",
				"fontStyle": ""
			}
		},
		/** 
		 * #########################
		 * # COMMENT-RELATED BLOCK #
		 * #########################
		 */
		{
			"name": "Single Line Comment",
			"scope": [
				"comment.line.double-slash",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7F9F7F"
			}
		},
		{
			"name": "Multi-Line Comment",
			"scope": [
				"comment.block"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#73879B"
			}
		},
		{
			"name": "Javadoc Comment",
			"scope": [
				"comment.block.documentation",
				"comment.block.javadoc.java",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#B3B5AF"
			}
		},
		{
			"name": "Javadoc Annotations",
			"scope": [
				"keyword.other.documentation.javadoc.java",
				"storage.type.class.doxygen.cpp",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#CC9393"
			}
		},
		{
			"name": "Annotations",
			"scope": [
				"storage.type.annotation",
				"punctuation.definition.annotation",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#808080"
			}
		},
		{
			"name": "TODO Highlight",
			"scope": [
				"comment.todo",
				"comment.other.radar",
				"comment.other.todo",
				"comment.other.fixme",
				"comment.other.changed",
				"comment.other.note"
			],
			"settings": {
				"foreground": "#ACC1AC",
				"fontStyle": "bold italic underline"
			}
		},
		{
			"name": "Javadoc Tag",
			"scope": [
				"comment.block.documentation.other.tag.html"
			],
			"settings": {
				"foreground": "#9393CC",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Javadoc Link",
			"scope": [
				"comment.block.documentation.other.javadoc.link",
				"comment.block.documentation.other.javadoc.code",
			],
			"settings": {
				"foreground": "#A893CC",
				"fontStyle": "italic"
			}
		},
		/**
		 * ###############################
		 * # STRINGS AND CONSTANT VALUES #
		 * ###############################
		 */
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.quoted",
				"string.quoted.double"
			],
			"settings": {
				"foreground": "#CC9393"
			}
		},
		{
			"name": "Literals / Numberic Values",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#EAB882"
			}
		},
		{
			"name": "Constant (True/False/Null/etc.)",
			"scope": [
				"constant.language"
			],
			"settings": {
				"foreground": "#8DCBE2",
				"fontStyle": "bold"
			}
		},
		/**
		 * ########################
		 * # MISCELLANEOUS BLOCKS #
		 * ########################
		 */
		{
			"name": "Deprecated Methods/Functions",
			"scope": [
				"*.deprecated",
				"entity.name.function.deprecated",
				"entity.name.method.deprecated"
			],
			"settings": {
				"foreground": "#FFFFFF",
				"fontStyle": "italic strikethrough"
			}
		},
	],
}